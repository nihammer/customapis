service cloud.firestore {
  match /databases/{database}/documents {
    function isFieldValueNotChanged(fieldName) {
      return request.resource.data[fieldName] == resource.data[fieldName];
    }
    match /test/{testId} {
      allow get;
      allow list;
      allow create: if request.resource.data.size() == 3;
      allow update: if isFieldValueNotChanged('test1')
        && isFieldValueNotChanged('test2')
        && isFieldValueNotChanged('test3');
      allow delete;
    }
  }
}
